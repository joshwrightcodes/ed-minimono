{
  "UseInMemoryDatabase": true,
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": {
      "Console": {
        "Name": "Console"
      }
    }
  },
  "OpenTelemetry": {
    "Zipkin": {
      "Endpoint":"http://localhost:9411/api/v2/spans"
    }
  }
}
